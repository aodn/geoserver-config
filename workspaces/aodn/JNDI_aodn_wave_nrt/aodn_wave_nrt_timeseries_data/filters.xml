        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>instrument_type</name>
        <type>string</type>
        <label>Instrument type</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>wmo_id</name>
        <type>string</type>
        <label>Wmo id</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
