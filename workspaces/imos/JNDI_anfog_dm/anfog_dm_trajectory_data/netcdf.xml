<?xml version="1.0"?>
<definition>
  <source>
    <dataStoreName>JNDI_anfog_dm</dataStoreName>
    <virtualDataTable>select file_id as instance_id, * from anfog_dm.anfog_dm_trajectory_data</virtualDataTable>
    <virtualInstanceTable>select id from anfog_dm.anfog_dm_trajectory_map</virtualInstanceTable>
  </source>
  <filename>
    <sql>
        select
        'IMOS_ANFOG_'
        ||to_char(min("TIME" AT TIME ZONE 'UTC'), 'YYYYMMDD"T"HH24MISS"Z"')
        ||'_'
        ||platform_code
        ||'_FV01_'
        ||deployment_name
        ||'_END-'
        ||to_char(max("TIME" AT TIME ZONE 'UTC'), 'YYYYMMDD"T"HH24MISS"Z"')
        ||'_id-'
        ||instance_id
        ||'.nc'
        from $data
        group by platform_code, voyage_number, instance_id
    </sql>
  </filename>
  <dimensions>
    <dimension name="TIME"/>
  </dimensions>
  <globalattributes>
    <attribute name="title" sql="SELECT platform_type ||' data from the mission '||deployment_name from $instance"/>
    <attribute name="file_version" value="'Level 1 - Quality Controlled Data'"/>
    <attribute name="abstract">
          <sql>
          select
          'This NetCDF file contains data from the '
          ||deployment_name
          ||' glider deployment measured with unit '
          ||platform_code
          ||' ('
          ||platform_type
          ||') between '
          ||to_char(min("TIME" AT TIME ZONE 'UTC'), 'YYYY-MM-DD"T"HH24:MI:SS"Z"')
          ||' and '
          ||to_char(max("TIME" AT TIME ZONE 'UTC'), 'YYYY-MM-DD"T"HH24:MI:SS"Z"')
          from $data
          group by deployment_name, platform_type, platform_code
      </sql>
    </attribute>
    <attribute name="project" value="'Integrated Marine Observing System (IMOS)'"/>
    <attribute name="references" value="'http://www.imos.org.au'"/>
    <attribute name="Conventions" value="'CF-1.6,IMOS-1.3'"/>
    <attribute name="institution" value="'Glider facility'"/>
    <attribute name="principal_investigator" value="'Prof Charitha Pattiaratchi'" />
    <attribute name="principal_investigator_email" value="'chari.pattiaratchi@uwa.edu.au'" />
    <attribute name="standard_name_vocabulary" value="'CF-1.6'"/>
    <attribute name="quality_control_set" type="byte" value="1" />
    <attribute name="date_created">
      <sql>select to_char(now() AT TIME ZONE 'UTC', 'YYYY-MM-DD"T"HH24:MI:SS"Z"')</sql>
    </attribute>
    <attribute name="platform_type" sql="SELECT platform_type FROM $instance"/>
    <attribute name="platform_code" sql="select platform_code from $instance"/>
    <attribute name="deployment_name" sql="select deployement_name from $instance"/>
    <attribute name="source" value="'Glider observation'"/>
    <attribute name="naming_authority" value="'IMOS'"/>
    <attribute name="geospatial_lat_units" value="'degrees_north'"/>
    <attribute name="geospatial_lon_units" value="'degrees_east'"/>
    <attribute name="geospatial_vertical_units" value="'meter'"/>
    <attribute name="geospatial_vertical_positive" value="'down'"/>
    <attribute name="geospatial_lat_min">
      <sql>select min("LATITUDE") from $data</sql>
    </attribute>
    <attribute name="geospatial_lat_max">
      <sql>select max("LATITUDE") from $data</sql>
    </attribute>
    <attribute name="geospatial_lon_min">
      <sql>select min("LONGITUDE") from $data</sql>
    </attribute>
    <attribute name="geospatial_lon_max">
      <sql>select max("LONGITUDE") from $data</sql>
    </attribute>
    <attribute name="geospatial_vertical_max">
      <sql>select max("DEPTH") from $data</sql>
    </attribute>
    <attribute name="geospatial_vertical_min">
      <sql>select min("DEPTH") from $data</sql>
    </attribute>
    <attribute name="time_coverage_start">
      <sql>select to_char(min("TIME" AT TIME ZONE 'UTC'), 'YYYY-MM-DD"T"HH24:MI:SS"Z"') from $data</sql>
    </attribute>
    <attribute name="time_coverage_end">
      <sql>select to_char(max("TIME" AT TIME ZONE 'UTC'), 'YYYY-MM-DD"T"HH24:MI:SS"Z"') from $data</sql>
    </attribute>
    <attribute name="data_centre" value="'Australian Ocean Data Network'"/>
    <attribute name="data_centre_email" value="'info@aodn.org.au'"/>
    <attribute name="institution_references" value="'http://www.imos.org.au/aodn.html'"/>
    <attribute name="citation">
      <value>"'The citation in a list of references is: "IMOS [year-of-data-download], [Title], [data-access-url], accessed [date-of-access]"'"</value>
    </attribute>
    <attribute name="acknowledgement">
      <value>"'Any users of IMOS data are required to clearly acknowledge the source of the material in the format: "Data was sourced from the Integrated Marine Observing System (IMOS) - an initiative of the Australian Government being conducted as part of the National Collaborative Research Infrastructure Strategy and and the Super Science Initiative."'"</value>
    </attribute>
    <attribute name="distribution_statement">
      <value>"'Data may be re-used, provided that related metadata explaining the data has been reviewed by the user, and the data is appropriately acknowledged. Data, products and services from IMOS are provided "as is" without any warranty as to fitness for a particular purpose.'"</value>
    </attribute>
  </globalattributes>
  <variables>
    <variable>
      <name>TIME</name>
      <encoder>time</encoder>
      <dimensions>
        <dimension name="TIME"/>
      </dimensions>
      <attributes>
        <attribute name="standard_name" value="'time'"/>
        <attribute name="long_name" value="'time from the CTD'"/>
        <attribute name="units" value="'days since 1950-01-01 00:00:00 UTC'"/>
        <attribute name="calendar" value="'gregorian'"/>
        <attribute name="quality_control_set" type="byte" value="1" />
        <attribute name="axis" value="'T'"/>
        <attribute name="valid_min" type="double" value="0.0"/>
        <attribute name="valid_max" type="double" value="90000.0"/>
        <attribute name="valid_max" type="double" value="99999.0"/>
        <attribute name="_FillValue"  type="double"  value="-999999."/>
        <attribute name="ancillary_variables" value="'TIME_quality_control'" />
      </attributes>
    </variable>
    <variable>
      <name>TIME_quality_control</name>
      <encoder>byte</encoder>
      <dimensions>
        <dimension name="TIME"/>
      </dimensions>
