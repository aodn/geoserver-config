        <attribute name="ancillary_variables" value="'LATITUDE_quality_control'" />
        <attribute name="quality_control_set" type="byte" value="1" />
        <attribute name="observation_type" value="'measured'" />
        <attribute name="reference_datum" value="'geographical coordinates, WGS84 projection'" />
