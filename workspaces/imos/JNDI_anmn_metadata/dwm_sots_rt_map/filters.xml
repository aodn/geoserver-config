<?xml version="1.0"?>
<filters>
    <filter>
        <name>platform_code</name>
        <type>string</type>
        <label>Platform code</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>deployment_code</name>
        <type>string</type>
        <label>Deployment code</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
