<?xml version="1.0"?>
<filters>
    <filter>
        <name>transmitter_id</name>
        <type>string</type>
        <label>Tag ID</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>tag_project_name</name>
        <type>string</type>
        <label>Tag project name</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>common_name</name>
        <type>string</type>
        <label>Species Name</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>installation_name</name>
        <type>string</type>
        <label>Installation name</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>station_name</name>
        <type>string</type>
        <label>Installation station name</label>
        <visualised>true</visualised>
        <excludedFromDownload>false</excludedFromDownload>
    </filter>
    <filter>
        <name>detection_timestamp</name>
        <type>datetime</type>
        <label>Time</label>
