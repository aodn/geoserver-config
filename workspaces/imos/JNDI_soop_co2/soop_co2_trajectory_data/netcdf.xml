<?xml version="1.0"?>
<definition>
  <source>
    <dataStoreName>JNDI_soop_co2</dataStoreName>
    <virtualDataTable>select file_id as instance_id, * from soop_co2.soop_co2_trajectory_data</virtualDataTable>
    <virtualInstanceTable>select file_id as instance_id,* from soop_co2.soop_co2_trajectory_map</virtualInstanceTable>
  </source>
  <filename>
    <sql>
        select
        'IMOS_SOOP-CO2_'
        ||to_char(min("TIME" AT TIME ZONE 'UTC'), 'YYYYMMDD"T"HH24MISS"Z"')
        ||'_'
        ||instance.platform_code
        ||'_FV01_'
        ||replace(data.cruise_id,'_','-')
        ||'_END-'
        ||to_char(max("TIME" AT TIME ZONE 'UTC'), 'YYYYMMDD"T"HH24MISS"Z"')
        ||'_id-'
        ||instance_id
        ||'.nc'
        from $data
        JOIN instance on instance.instance_id=data.instance_id
        group by platform_code, data.cruise_id, instance_id
    </sql>
  </filename>
  <dimensions>
    <dimension name="TIME"/>
    <dimension name="string_3" length="3"/>
  </dimensions>
  <globalattributes>
    <attribute name="title">
      <sql> select 'IMOS Underway CO2 dataset measured onboard the '|| vessel_name ||' during the voyage ' || cruise_id from $instance</sql>
    </attribute>
    <attribute name="file_version" value="'Level 1 - Quality Controlled Data'"/>
    <attribute name="abstract">
      <sql>
          select
          'This dataset contains underway CO2 measurements collected onboard the '
          ||vessel_name
          ||' during the voyage '
          ||cruise_id
          ||' from '
          ||to_char(min("TIME" AT TIME ZONE 'UTC'), 'YYYY-MM-DD"T"HH24:MI:SS"Z"')
          ||' to '
          ||to_char(max("TIME" AT TIME ZONE 'UTC'), 'YYYY-MM-DD"T"HH24:MI:SS"Z"')
          from $data
          group by vessel_name, cruise_id
      </sql>
    </attribute>
    <attribute name="project" value="'Integrated Marine Observing System (IMOS)'"/>
    <attribute name="Conventions" value="'CF-1.6,IMOS-1.3'"/>
    <attribute name="institution">
      <sql>
      SELECT platform_code,
        CASE WHEN platform_code='ZMFR' THEN
             'NIWA, New Zealand'
             ELSE
             'Enhanced Measurements from Ships of Opportunity (SOOP) - CO2'
        END
      FROM $instance
      </sql>
    </attribute>
    <attribute name="standard_name_vocabulary" value="'CF-1.6'"/>
    <attribute name="naming_authority" value="'IMOS'"/>
    <attribute name="quality_control_set" type="byte" value="4"/>
    <attribute name="source" value="ship observation"/>
    <attribute name="date_created">
      <sql>select to_char(now() AT TIME ZONE 'UTC', 'YYYY-MM-DD"T"HH24:MI:SS"Z"')</sql>
    </attribute>
    <attribute name="references" value="'http://www.imos.org.au'"/>
    <attribute name="vessel_name" sql="SELECT vessel_name FROM $instance"/>
    <attribute name="platform_code" sql="select platform_code from $instance"/>
    <attribute name="cruise_id" sql="select cruise_id from $instance"/>
    <attribute name="featureType" value="'trajectory'"/>
    <attribute name="geospatial_lat_min">
      <sql>select min("LATITUDE") from $data</sql>
    </attribute>
    <attribute name="geospatial_lat_max">
      <sql>select max("LATITUDE") from $data</sql>
    </attribute>
    <attribute name="geospatial_lon_min">
      <sql>select min("LONGITUDE") from $data</sql>
    </attribute>
    <attribute name="geospatial_lon_max">
      <sql>select max("LONGITUDE") from $data</sql>
    </attribute>
    <attribute name="geospatial_vertical_min" value="0"/>
    <attribute name="geospatial_vertical_max" value="0"/>
    <attribute name="time_coverage_start">
      <sql>select to_char(min("TIME" AT TIME ZONE 'UTC'), 'YYYY-MM-DD"T"HH24:MI:SS"Z"') from $data</sql>
    </attribute>
    <attribute name="time_coverage_end">
      <sql>select to_char(max("TIME" AT TIME ZONE 'UTC'), 'YYYY-MM-DD"T"HH24:MI:SS"Z"') from $data</sql>
    </attribute>
    <attribute name="data_centre" value="'Australian Ocean Data Network (AODN)'"/>
    <attribute name="data_centre_email" value="'info@aodn.org.au'"/>
    <attribute name="institution_references">
      <sql>
      SELECT platform_code,
        CASE WHEN platform_code='ZMFR' THEN
             'http://www.niwa.co.nz/atmosphere/programme-overview/oceanic-control-of-atmospheric-composition'
             ELSE
            'http://www.imos.org.au/aodn.html'
        END
      FROM $instance
      </sql>
    </attribute>
    <attribute name="principal_investigator">
      <sql>
      SELECT platform_code,
        CASE WHEN platform_code='ZMFR' THEN
             'Dr Kim Currie, NIWA'
             ELSE
             'Tilbrook, Bronte, CSIRO; Neill, Craig, CSIRO; Akl, John, CSIRO'
        END
      FROM $instance
      </sql>
    </attribute>
    <attribute name="citation">
      <sql>
      SELECT
        'The citation in a list of references is: IMOS, [year-of-data-download], IMOS Underway CO2 dataset measured on the '
        ||vessel_name
        ||' voyage '
        ||cruise_id
        ||', [data-access-URL], accessed [date-of-access]'
      FROM $instance
      </sql>
    </attribute>
    <attribute name="acknowledgement">
      <sql>
      SELECT platform_code,
        CASE WHEN (platform_code='VLMJ' OR platform_code='VLHJ') THEN
             'Any users of IMOS data are required to clearly acknowledge the source of the material in the format: "Data was sourced from the Integrated Marine Observing System (IMOS) - IMOS is supported by the Australian Government through the National Collaborative Research Infrastructure Strategy (NCRIS) and the Super Science Initiative (SSI). The science support groups of the Australian Marine National Facility and CSIRO, are thanked for their expertise in helping to setup and collect the data."'
             WHEN platform_code='ZMFR' THEN
             'This data was collected with funding from the Atmosphere and Climate Centre, NIWA, New Zealand. Post-processing software courtesy Denis Pierrot, NOAA'
             ELSE
             'Any users of IMOS data are required to clearly acknowledge the source of the material in the format: "Data was sourced from the Integrated Marine Observing System (IMOS) - IMOS is supported by the Australian Government through the National Collaborative Research Infrastructure Strategy (NCRIS) and the Super Science Initiative (SSI)."'
        END
        FROM $instance
      </sql>
    </attribute>
    <attribute name="distribution_statement">
      <value>'Data may be re-used, provided that related metadata explaining the data has been reviewed by the user, and the data is appropriately acknowledged. Data, products and services from IMOS are provided "as is" without any warranty as to fitness for a particular purpose.'</value>
    </attribute>
  </globalattributes>
