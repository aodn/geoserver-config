<gogoduck>
  <fileLimit>10</fileLimit>
  <threadCount>1</threadCount>
  <urlSubstitution key="^">http://data.aodn.org.au/</urlSubstitution>
  <ncksPath>/usr/bin/ncks</ncksPath>
  <ncpdqPath>/usr/bin/ncpdq</ncpdqPath>
  <ncrcatPath>/usr/bin/ncrcat</ncrcatPath>
  <ncattedPath>/usr/bin/ncatted</ncattedPath>
  <timeField>time</timeField>
  <sizeField>size</sizeField>
  <fileSizeLimit></fileSizeLimit>
  <fileUrlField>file_url</fileUrlField>
  <unpack>true</unpack>
  <globalAttributes>
    <title>title</title>
    <latitudeStart>geospatial_lat_min</latitudeStart>
    <latitudeEnd>geospatial_lat_max</latitudeEnd>
    <longitudeStart>geospatial_lon_min</longitudeStart>
    <longitudeEnd>geospatial_lon_max</longitudeEnd>
    <timeStart>time_coverage_start</timeStart>
    <timeEnd>time_coverage_end</timeEnd>
  </globalAttributes>
  <!-- aggregation templates -->
    <templates>
      <template match=".*bathy_ppb_deakin.*">bathymetry</template>
      <template match="srs_ghrsst.l3*">srs_ghrsst_l3</template>
      <template match="srs_ghrsst.l4_*">srs_ghrsst_l4</template>
      <template match="srs_sst.*">srs_ghrsst_l3</template>
      <template match=".*">default</template>
    </templates>
</gogoduck>
